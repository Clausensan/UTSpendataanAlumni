<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PendataanAlumni"
        tools:targetApi="31" >
        <activity
            android:name=".DetailDataActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_data"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".DataAlumniActivity"
            android:exported="false"
            android:label="@string/title_activity_data_alumni"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".TambahDataActivity"
            android:exported="false"
            android:label="@string/title_activity_tambah_data"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".DetailNewsActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_news"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".BeritaActivity"
            android:exported="false"
            android:label="@string/title_activity_berita"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.PendataanAlumni" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".home"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>